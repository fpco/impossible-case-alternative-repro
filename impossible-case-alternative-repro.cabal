name:                impossible-case-alternative-repro
version:             0.0
build-type:          Simple
cabal-version:       >=1.8

executable impossible-case-alternative-repro
  main-is:        Main.hs
  ghc-options:    -Wall
  build-depends:
      base >= 4 && <5
    , aeson
    , transformers-compat
    , transformers
    , mtl
    , template-haskell

